    "generate-apk": "java -jar C:/bundletool.jar build-apks --mode=universal --bundle=android/app/build/outputs/bundle/release/app.aab --output=wallet.apks --ks=android/app/limonbyte.keystore --ks-pass=pass:limonbyte --ks-key-alias=limonbyte --key-pass=pass:limonbyte",
